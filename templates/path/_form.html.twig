<div id="model_form">
  {{ form_errors(form) }}

  {{ form_start(form) }}

  {{ form_row(form.name) }}
  {{ form_row(form.price) }}
  {{ form_row(form.manufacture) }}
  {{ form_row(form.colors) }}

  <button class="btn btn-primary" type="submit">{{ 'btn.save' | trans }}</button>
  {{ form_end(form) }}
</div>
